<?php

function nibai($a) {
  echo 2 * $a;
}

nibai(3);
echo "\n";
nibai(5);
echo "\n";

function f($a, $b) {
 return $a + $b;
}

$sum = f(2,3);
echo $sum;

echo "\n";

function a($arr) {
 $result = 1;
 foreach ($arr as $a) {
 $result *= $a;
 }
 echo $result;
}

a(array(1,3,5,7,9));

echo "\n";

function max_array($arr){
 $max_number = 1;
 foreach($arr as $a) {
 if($max_number < $a) {
    $max_number = $a;
  }    
 } 
 echo $max_number;
}
max_array([77,12,25,69,]);
 
echo "\n";